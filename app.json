{
  "expo": {
    "name": "ONCF Voyages",
    "slug": "ONCF-Voyages",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/al-boraq-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/al-boraq-logo.png",
        "backgroundColor": "transparent"
      },
      "package": "com.ismailfromthespace.ONCFVoyages"
    },
    "web": {
      "favicon": "./assets/images/al-boraq-logo.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "0a87f67f-6ffc-465c-a79e-8d591395b691"
      }
    }
  }
}
